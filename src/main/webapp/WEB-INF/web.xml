<web-app>
    <!-- ScoreDeleteServlet -->
    <servlet>
        <servlet-name>ScoreDelete</servlet-name>
        <servlet-class>ScoreDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScoreDelete</servlet-name>
        <url-pattern>/deleteScore</url-pattern>
    </servlet-mapping>

    <!-- ScoreListServlet -->
    <servlet>
        <servlet-name>ScoreList</servlet-name>
        <servlet-class>ScoreListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScoreList</servlet-name>
        <url-pattern>/listScore</url-pattern>
    </servlet-mapping>

    <!-- ScoreRegisterServlet -->
    <servlet>
        <servlet-name>ScoreRegister</servlet-name>
        <servlet-class>ScoreRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScoreRegister</servlet-name>
        <url-pattern>/registerScore</url-pattern>
    </servlet-mapping>

    <!-- ScoreUpdateServlet -->
    <servlet>
        <servlet-name>ScoreUpdate</servlet-name>
        <servlet-class>ScoreUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScoreUpdate</servlet-name>
        <url-pattern>/updateScore</url-pattern>
    </servlet-mapping>

    <!-- StudentAction -->
    <servlet>
        <servlet-name>StudentAction</servlet-name>
        <servlet-class>StudentAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentAction</servlet-name>
        <url-pattern>/studentAction</url-pattern>
    </servlet-mapping>

    <!-- StudentDeleteServlet -->
    <servlet>
        <servlet-name>StudentDelete</servlet-name>
        <servlet-class>StudentDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentDelete</servlet-name>
        <url-pattern>/deleteStudent</url-pattern>
    </servlet-mapping>

    <!-- StudentRegistration -->
    <servlet>
        <servlet-name>StudentRegistration</servlet-name>
        <servlet-class>StudentRegistration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentRegistration</servlet-name>
        <url-pattern>/registerStudent</url-pattern>
    </servlet-mapping>

    <!-- StudentRegistrationServlet -->
    <servlet>
        <servlet-name>StudentRegistrationServlet</servlet-name>
        <servlet-class>StudentRegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentRegistrationServlet</servlet-name>
        <url-pattern>/registerStudentServlet</url-pattern>
    </servlet-mapping>

    <!-- StudentSearchServlet -->
    <servlet>
        <servlet-name>StudentSearch</servlet-name>
        <servlet-class>StudentSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentSearch</servlet-name>
        <url-pattern>/searchStudent</url-pattern>
    </servlet-mapping>

    <!-- StudentUpdateServlet -->
    <servlet>
        <servlet-name>StudentUpdate</servlet-name>
        <servlet-class>StudentUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentUpdate</servlet-name>
        <url-pattern>/updateStudent</url-pattern>
    </servlet-mapping>
</web-app>
